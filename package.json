{
  "name": "junction-normalizr-decorator",
  "version": "0.2.4",
  "description": "Map your Junction entity schema to an output suitable for normalizr",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/andy-shea/junction-normalizr-decorator"
  },
  "homepage": "https://github.com/andy-shea/junction-normalizr-decorator",
  "browserslist": [
    "ie >= 11"
  ],
  "scripts": {
    "build": "babel --extensions '.ts' src -d lib && yarn types:build",
    "prepublish": "yarn build",
    "test": "cross-env NODE_ENV=test nyc tape -r ts-node/register ./test/**/*.ts",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prebuild": "yarn test",
    "types:build": "tsc --emitDeclarationOnly",
    "types:check": "tsc --noEmit"
  },
  "keywords": [
    "orm",
    "pojo",
    "mapper",
    "junction",
    "decorator",
    "normalizr",
    "schema"
  ],
  "author": "Andy Shea",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-syntax-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/pluralize": "^0.0.29",
    "@types/tape": "^4.2.32",
    "babel-plugin-istanbul": "^4.1.3",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.0",
    "nyc": "^10.3.2",
    "tape": "^4.6.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "normalizr": "^3.2.3",
    "pluralize": "^5.0.0"
  }
}
